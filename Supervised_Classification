              "Value": 3,
              "system:index": "75"
            })]),
    L5_SR = ee.ImageCollection("LANDSAT/LT05/C02/T1_L2");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var L8_SR_1=L8_SR.filterBounds(watershed).filterDate('2020-03-01','2020-05-30').sort('CLOUD_COVER')
            .filter(ee.Filter.lt('CLOUD_COVER', 1));
var L8_SR_2=L8_SR.filterBounds(geometry).filterDate('2020-01-01','2020-05-30').sort('CLOUD_COVER').first();
var L8_SR_M=L8_SR_1.merge(L8_SR_2);          
print(L8_SR_M);
print(L8_SR_M.aggregate_array('DATE_ACQUIRED'));
print(L8_SR_M.aggregate_array('CLOUD_COVER'));

Map.centerObject(watershed,8)
Map.addLayer(watershed,{color:'Black'},'watershed');

L8_SR_M=L8_SR_M.mean();
L8_SR_M=(L8_SR_M.select('SR_B[1-7]').multiply(2.75e-05)).add(-0.2)
Map.addLayer(L8_SR_M,TCC,'TCC');
Map.addLayer(L8_SR_M,FCC,'FCC');

var classes=WATER_BODY.merge(AGRICULTURE_LAND).merge(FOREST).merge(URBAN_AREA);
print(classes)


var bands = ['SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B6', 'SR_B7'];
var training = L8_SR_M.select(bands).sampleRegions({
  collection: classes,
  properties: ['Value'],
  scale: 30
});
print(training);


var classifier = ee.Classifier.smileCart().train({
  features: training,
  classProperty: 'Value',
  inputProperties: bands
});

var classified = L8_SR_M.select(bands).classify(classifier);

//Display classification
// Map.centerObject(classes, 11);
Map.addLayer(classified,
{min: 0, max: 3, palette: ['Blue', 'White', 'Green','Red']},
'classification');

var LULC_2020=classified.clip(watershed).rename('LULC_2020');
Map.addLayer(LULC_2020,
{min: 0, max: 3, palette: ['Blue', 'White', 'Green','Red']},
'LULC_2020');


// 2010
var L5_SR_1=L5_SR.filterBounds(watershed).filterDate('2010-01-01','2010-07-30').sort('CLOUD_COVER')
            .filter(ee.Filter.lt('CLOUD_COVER', 5));
var L5_SR_2=L5_SR.filterBounds(geometry).filterDate('2010-01-01','2010-07-30').sort('CLOUD_COVER').first();
var L5_SR_M=L5_SR_1.merge(L5_SR_2);          
print(L5_SR_M);
print(L5_SR_M.aggregate_array('DATE_ACQUIRED'));
print(L5_SR_M.aggregate_array('CLOUD_COVER'));

L5_SR_M=L5_SR_M.mean();
L5_SR_M=(L5_SR_M.select('SR_B[1-7]').multiply(2.75e-05)).add(-0.2)
Map.addLayer(L5_SR_M,TCC,'TCC');
Map.addLayer(L5_SR_M,FCC,'FCC');

var bands = ['SR_B1','SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B7'];
var training = L5_SR_M.select(bands).sampleRegions({
  collection: classes,
  properties: ['Value'],
  scale: 30
});
print(training);


var classifier = ee.Classifier.smileCart().train({
  features: training,
  classProperty: 'Value',
  inputProperties: bands
});

var classified = L5_SR_M.select(bands).classify(classifier);

//Display classification
// Map.centerObject(classes, 11);
Map.addLayer(classified,
{min: 0, max: 3, palette: ['Blue', 'White', 'Green','Red']},
'classification');

var LULC_2010=classified.clip(watershed).rename('LULC_2010');
Map.addLayer(LULC_2010,
{min: 0, max: 3, palette: ['Blue', 'White', 'Green','Red']},
'LULC_2010');

